{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "author": "sliver",
  "description": "gui for cli",
  "main": "main.js",
  "scripts": {
    "build-renderer": "webpack --progress --config webpack.prod.js",
    "start-renderer": "webpack-dev-server --config webpack.dev.js",
    "build-react": "webpack --progress --config webpack.react.js",
    "electron": "electron .",
    "bundle": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "test": "mocha"
  },
  "build": {
    "appId": "sliver.gui",
    "copyright": "sliver",
    "productName": "sliver-gui",
    "files": [
      "./dist/**/*",
      "./main.js",
      "./main_process/**/*",
      "./node_modules/**/*",
      "./preload.js",
      "./public/**/*"
    ],
    "mac": {
      "category": "sliver.gui.category"
    },
    "win": {
      "icon": "public/favicon.ico"
    },
    "directories": {
      "output": "bundle"
    }
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.1",
    "consolidate": "^0.16.0",
    "download-git-repo": "^3.0.2",
    "electron": "^10.2.0",
    "iconv-lite": "^0.6.2",
    "jquery": "^3.5.1",
    "metalsmith": "^2.3.0",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/plugin-transform-react-display-name": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^11.1.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.0.0",
    "electron-reload": "^1.5.0",
    "electron-webpack": "^2.8.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^8.2.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "sass-loader": "^10.0.4",
    "spectron": "^12.0.0",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
